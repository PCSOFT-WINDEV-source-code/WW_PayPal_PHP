      procedure public SetReturnPagePaymentCanceled(sReturnPage)
      
      gsPageReturnCancel = sReturnPage
      
      if gsPageReturnCancel = "" then
      	gsCancelReturn = ""
      else
      	gsCancelReturn = PageAddress("", paFULL) + "?WD_BUTTON_CLICK_="  + BTN_RETURNCANCEL..Alias
      end
      
