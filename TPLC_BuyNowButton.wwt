     name : SetCurrency
     procedure_id : 1157103063402895480
     type_code : 14
     code : |1+
      // Summary: Defines the payment currency
      // Syntax:
      //SetCurrency (<nCurrency>)
      //
      // Parameters:
      //	nCurrency: Currency (eCZK, eDKK, eEUR, eGBP, eHUF, ePLN, eSEK, eUSD or eJPY constant)
      // Return value:
      // 	None
      
      procedure public SetCurrency(nCurrency)
      
      switch nCurrency
      	//		CASE eCZK: gsCurrencyCode = "CZK"
      	case eDKK : gsCurrencyCode = "DKK"
      	case eEUR : gsCurrencyCode = "EUR"
      	case eGBP : gsCurrencyCode = "GBP"
      		//		CASE eHUF: gsCurrencyCode = "HUF"
      	//		CASE ePLN: gsCurrencyCode = "PLN"
      	case eSEK : gsCurrencyCode = "SEK"
      	case eUSD : gsCurrencyCode = "USD"			
      	case eJPY : gsCurrencyCode = "JPY"
      end
      
      _HTML()
