      // Summary: Defines the language for the PayPal site 
      // Syntax:
      //SetLanguage (<nLanguage>)
      //
      // Parameters:
      //	nLanguage: Language to use
      // Return value:
      // 	None
      
      procedure public SetLanguage(nLanguage)
      
      switch nLanguage
      	case languageEnglish : gsLanguage 	= "GB"    //English
      	case languageSpanish : gsLanguage = "ES" 	//Spanish
      	case languageFrench : gsLanguage = "FR" 	//French
      	case languageChinese : gsLanguage = "CN" 	//Chinese
      end
      
      
